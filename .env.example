# Environment Configuration
# Copy this file to .env and configure your settings

# PasskeyMe Configuration
VITE_PASSKEYME_APP_ID=your-app-id-here
VITE_PASSKEYME_BASE_URL=https://passkeyme.com
VITE_PASSKEYME_REDIRECT_URI=http://localhost:3000/callback
VITE_PASSKEYME_PASSKEY_API_KEY=your-passkey-api-key

# Development Settings
VITE_DEBUG_MODE=true
VITE_ENVIRONMENT=development

# Optional: Custom Branding
VITE_APP_NAME=PasskeyMe Demo
VITE_APP_DESCRIPTION=Experience seamless authentication with PasskeyMe

# Production Settings (for deployment)
# VITE_PASSKEYME_BASE_URL=https://passkeyme.com
# VITE_PASSKEYME_REDIRECT_URI=https://demo.passkeyme.com/callback
# VITE_DEBUG_MODE=false
# VITE_ENVIRONMENT=production
