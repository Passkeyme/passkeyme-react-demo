    "authentication",
    "react",
    "webauthn",
    "demo",
    "sdk",
    "oauth",
    "biometric"
  ],
  "homepage": "https://demo.passkeyme.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/passkeyme/passkeyme-react-demo.git"
  },
  "bugs": {
    "url": "https://github.com/passkeyme/passkeyme-react-demo/issues"
  },
  "license": "MIT",
  "author": {
    "name": "PasskeyMe",
    "email": "hello@passkeyme.com",
    "url": "https://passkeyme.com"
  },
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix"
  },
  "dependencies": {
    "@passkeyme/auth": "1.1.7",
    "@passkeyme/react-auth": "1.1.7",
